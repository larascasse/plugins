<config>
    <modules>
        <Creadev_Iadvize>
            <version>1.0.0</version>
        </Creadev_Iadvize>
    </modules>

    <global>
	
        <models>
            <iadvize><class>Creadev_Iadvize_Model</class></iadvize>
        </models>

        <blocks>
            <iadvize>
				<class>Creadev_Iadvize_Block</class>
			</iadvize>
        </blocks>
	
        <helpers>
            <iadvize><class>Creadev_Iadvize_Helper</class></iadvize>
        </helpers>
	
    </global>

    <frontend>
		<!--
        <translate>
            <modules>
                <Creadev_iAdvize>
                    <files>
                        <default>Creadev_iAdvize.csv</default>
                    </files>
                </Creadev_iAdvize>
            </modules>
        </translate>
		
        		
		-->
        <layout>
            <updates>
                <iadvize>
                    <file>iadvize.xml</file>
                </iadvize>
            </updates>
        </layout>
    </frontend>

</config>
		